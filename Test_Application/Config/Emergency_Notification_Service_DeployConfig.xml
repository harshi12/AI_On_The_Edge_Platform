<Service name = "Emergency_Notification_Service">
	<SetUpEnvironment>
		<Install>None</Install>
	</SetUpEnvironment>

	<Criticality>No</Criticality>
	
	<Exclusive>No</Exclusive>
	
	<MinimumInstances>2</MinimumInstances>
	
	<FolderPath>Services/Emergency_Notification_Service/</FolderPath>
	
	<ExecutableFileName>emergency_notification_service.py</ExecutableFileName>
	
	<Dependencies>
		<Depedency>mailbox.py</Depedency>
	</Dependencies>
	
	
	<UI>
		<FolderPath></FolderPath>
		<ExecutableFileName></ExecutableFileName>
	</UI>
	
	<Scheduling>
		<!-- TriggerInterval is in seconds -->
		<TriggerInterval>None</TriggerInterval>
		<!-- If StartTime is None then launch service immediately -->
		<StartTime>None</StartTime>
		<!-- Disabling Mechanism specified by EndTime -->
		<EndTime>2359</EndTime>
		<UpTime>None</UpTime>
	</Scheduling>

	
</Service>